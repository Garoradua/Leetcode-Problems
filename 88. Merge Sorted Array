class Solution {
    public void merge(int[] nums1, int m, int[] nums2, int n) {
        for(int j=0, i=m ; j<n; j++){
            nums1[i] = nums2[j];
            i++;
        }
        Arrays.sort(nums1);
        // int i=0, j=0;
        // int end = totalLength -1;
        // while(j < n){
        //     if(nums1[i]<=nums2[j]){
        //         i++;
        //     }else{
        //         end = totalLength -1;
        //         while(end > i){
        //             nums1[end] = nums1[end-1];
        //             end--;
        //         }
        //         nums1[i] = nums2[j];
        //         i++;
        //         j++;
        //     }
        // }
        // while(j <n ){
        //     nums1[i] = nums2[j];
        //     i++;
        //     j++;
        // }
    }
}
